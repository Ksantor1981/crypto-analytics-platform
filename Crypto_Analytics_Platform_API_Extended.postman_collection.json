                {"key": "limit", "value": "10"},
                {"key": "days", "value": "30"}
              ]
            }
          }
        },
        {
          "name": "Get Telegram Signals",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/signals/telegram?skip=0&limit=100&symbol=BTC&signal_type=buy&source=channel1",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "signals", "telegram"],
              "query": [
                {"key": "skip", "value": "0"},
                {"key": "limit", "value": "100"},
                {"key": "symbol", "value": "BTC"},
                {"key": "signal_type", "value": "buy"},
                {"key": "source", "value": "channel1"}
              ]
            }
          }
        },
        {
          "name": "Create Telegram Signals Batch",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"}
            ],
            "body": {
              "mode": "raw",
              "raw": "[\n  {\n    \"symbol\": \"BTC/USDT\",\n    \"signal_type\": \"buy\",\n    \"entry_price\": 50000.0,\n    \"target_price\": 55000.0,\n    \"stop_loss\": 48000.0,\n    \"confidence\": 0.85,\n    \"source\": \"telegram_channel_1\",\n    \"metadata\": {\"leverage\": \"10x\"}\n  }\n]"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/signals/telegram/batch",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "signals", "telegram", "batch"]
            }
          }
        }
      ]
    },
