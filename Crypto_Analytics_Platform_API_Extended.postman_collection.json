{
  "info": {
    "name": "Crypto Analytics Platform API - Extended",
    "description": "Extended endpoints for Channels, Signals, Subscriptions, Payments, ML",
    "version": "1.0.0"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8000"
    },
    {
      "key": "accessToken",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "Channels",
      "item": [
        {
          "name": "Get All Channels",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/channels?skip=0&limit=100&platform=telegram&category=crypto",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "channels"],
              "query": [
                {"key": "skip", "value": "0"},
                {"key": "limit", "value": "100"},
                {"key": "platform", "value": "telegram"},
                {"key": "category", "value": "crypto"}
              ]
            }
          }
        },
        {
          "name": "Get Channel by ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/channels/1",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "channels", "1"]
            }
          }
        },
        {
          "name": "Get Channel Signals",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/channels/1/signals?skip=0&limit=100",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "channels", "1", "signals"],
              "query": [
                {"key": "skip", "value": "0"},
                {"key": "limit", "value": "100"}
              ]
            }
          }
        },
        {
          "name": "Get Channel Statistics",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/channels/1/statistics",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "channels", "1", "statistics"]
            }
          }
        }
      ]
    },
    {
      "name": "Signals",
      "item": [
        {
          "name": "Get All Signals",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/signals?skip=0&limit=100&channel_id=1&asset=BTC/USDT&direction=BUY&status=ACTIVE",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "signals"],
              "query": [
                {"key": "skip", "value": "0"},
                {"key": "limit", "value": "100"},
                {"key": "channel_id", "value": "1"},
                {"key": "asset", "value": "BTC/USDT"},
                {"key": "direction", "value": "BUY"},
                {"key": "status", "value": "ACTIVE"}
              ]
            }
          }
        },
        {
          "name": "Get Signal by ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/signals/1",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "signals", "1"]
            }
          }
        },
        {
          "name": "Get Signal Stats Overview",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/signals/stats/overview?channel_id=1&days=30",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "signals", "stats", "overview"],
              "query": [
                {"key": "channel_id", "value": "1"},
                {"key": "days", "value": "30"}
              ]
            }
          }
        },
        {
          "name": "Get Channel Signal Stats",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/signals/stats/channel/1?days=30",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "signals", "stats", "channel", "1"],
              "query": [
                {"key": "days", "value": "30"}
              ]
            }
          }
        },
        {
          "name": "Get Top Performing Signals",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/signals/analytics/top-performing?limit=10&days=30",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "signals", "analytics", "top-performing"],
